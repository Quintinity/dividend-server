{
  "name": "dividend-server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "npm run build-ts && npm run tslint",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json src",
    "test": "jest",
    "start": "node dist/server.js",
    "buildrun": "npm run build && npm run start"
  },
  "author": "Vlad Marica",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.16.3",
    "node-schedule": "^1.3.0",
    "sqlite3": "^4.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/node-schedule": "^1.2.2",
    "@types/sqlite3": "^3.1.3",
    "jest": "^23.4.1",
    "string-format": "^2.0.0",
    "ts-jest": "^23.0.1",
    "ts-lint": "^4.5.1",
    "typescript": "^2.9.2"
  },
  "jest": {
    "roots": [
      "test"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
